(IF (AND (volume_up)
     (AndS VELOCITY S_OVERBOUGHT P_M5)
     (AndS WPR_14 S_OVERBOUGHT P_M1 )
    )
 (START)
 (BUY)
 (IF (> T_PROFITSELL 0)
  (CLOSE_SELL)
 )
 (IF (= T_SELLNBRTRADE 0)
  (SET T_DIRECTION D_ANY)
 )
)
(IF (AND (volume_down)
     (AndS VELOCITY S_OVERSOLD P_M5)
     (AndS WPR_14 S_OVERSOLD P_M1)
    )
 (START)
 (SELL)
 (IF (> T_PROFITBUY 0)
  (CLOSE_BUY)
 )
 (IF (= T_SELLNBRTRADE 0)
  (SET T_DIRECTION D_ANY)
 )
)
